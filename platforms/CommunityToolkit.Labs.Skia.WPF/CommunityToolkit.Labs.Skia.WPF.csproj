  <PropertyGroup>
    <IsGtkHead>False</IsGtkHead>
    <IsWpfHead>True</IsWpfHead>
    <IsWpfLib>True</IsWpfLib>
    <IsWasmHead>False</IsWasmHead>
    <IsDeployableHead>true</IsDeployableHead>
    <TargetFramework>$(WpfLibTargetFramework)</TargetFramework>
