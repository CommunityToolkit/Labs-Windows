<Project Sdk="MSBuild.Sdk.Extras/3.0.23">
  <!-- Labs Constants -->
  <Import Project="$(RepositoryDirectory)common\Labs.TargetFrameworks.props" />
  <Import Project="$(RepositoryDirectory)common\Labs.ProjectIdentifiers.props" />

  <!-- Labs Platform Config -->
  <Import Project="$(RepositoryDirectory)common\Labs.Uno.props" />

  <!-- Labs Project Config -->
  <Import Project="$(RepositoryDirectory)common\MultiTarget\MultiTarget.props" />

  <PropertyGroup>
    <RootNamespace>CommunityToolkit.Labs.WinUI.TokenViewRns</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>CommunityToolkit.Labs.$(PackageIdVariant).TokenView</PackageId>
    <Description>
      This package contains TokenView.
    </Description>
    <Version>0.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="TokenView.xaml" />
    <UpToDateCheckInput Remove="Token\Token.xaml" />
  </ItemGroup>

  <ItemGroup>
    <SourceGeneratorInput Remove="TokenView\TokenView.ItemsSource.cs" />
  </ItemGroup>

  <!-- XAML Pages are automatically included, and don't need to be specified here. -->
</Project>
