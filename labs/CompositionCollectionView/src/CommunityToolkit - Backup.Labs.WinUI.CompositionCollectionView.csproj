<Project Sdk="MSBuild.Sdk.Extras/3.0.23">
  <Import Project="$(RepositoryDirectory)common\Labs.MultiTarget.props" />
  <Import Project="Dependencies.WinUI2.props" />
  <Import Project="Dependencies.WinUI3.props" />

  <PropertyGroup>
    <RootNamespace>CommunityToolkit.Labs.WinUI.CompositionCollectionViewRns</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>CommunityToolkit.Labs.$(PackageIdVariant).CompositionCollectionView</PackageId>
    <Description>
      This package contains CompositionCollectionView.
    </Description>
    <Version>0.0.2</Version>
    <Nullable>warnings</Nullable>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|uap10.0.17763|AnyCPU'">
    <DefineConstants></DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|uap10.0.17763|AnyCPU'">
    <DefineConstants></DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Page Include="**\*.xaml" Exclude="bin\**\*.xaml;obj\**\*.xaml" />
    <Compile Update="**\*.xaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Include="**\*.xaml" Exclude="bin\**\*.xaml;obj\**\*.xaml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="obj\bin\**" />
    <Content Remove="obj\bin\**" />
    <EmbeddedResource Remove="obj\bin\**" />
    <None Remove="obj\bin\**" />
    <Page Remove="obj\bin\**" />
    <UpToDateCheckInput Remove="obj\bin\**" />
  </ItemGroup>
  <ItemGroup>
    <None Include="CompositionCollectionView.shproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Behaviors\ElementInteractionTrackerBehavior.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
    <Compile Update="Behaviors\InteractionTrackerBehavior.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
    <Compile Update="Behaviors\InteractionTrackerGesture\GesturePreviewControl.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
    <Compile Update="Behaviors\InteractionTrackerGesture\InteractionTrackerGesture.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
    <Compile Update="Behaviors\LayoutBehavior.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
  </ItemGroup>
</Project>
