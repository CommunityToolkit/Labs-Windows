<Project Sdk="MSBuild.Sdk.Extras/3.0.23">
  <!-- Labs Constants -->
  <Import Project="$(RepositoryDirectory)common\Labs.TargetFrameworks.props" />
  <Import Project="$(RepositoryDirectory)common\Labs.ProjectIdentifiers.props" />

  <!-- Labs Platform Config -->
  <Import Project="$(RepositoryDirectory)common\Labs.Uno.props" />

  <!-- Labs Project Config -->
  <Import Project="$(RepositoryDirectory)common\Labs.MultiTarget.props" />

  <PropertyGroup>
    <RootNamespace>CommunityToolkit.Labs.WinUI.CompositionCollectionViewRns</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>CommunityToolkit.Labs.$(PackageIdVariant).CompositionCollectionView</PackageId>
    <Description>
      This package contains CompositionCollectionView.
    </Description>
    <Version>0.0.24</Version>
    <Nullable>warnings</Nullable>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|netstandard2.0|AnyCPU'">
    <NoWarn>WMC1006;CS8034;Uno0001</NoWarn>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netstandard2.0|AnyCPU'">
    <NoWarn>WMC1006;CS8034;Uno0001</NoWarn>
  </PropertyGroup>

  <!-- XAML Pages are automatically included, and don't need to be specified here. -->
  <ItemGroup>
    <Compile Update="Behaviors\ElementInteractionTrackerBehavior.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
    <Compile Update="Behaviors\InteractionTrackerBehavior.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
    <Compile Update="Behaviors\InteractionTrackerGesture\GesturePreviewControl.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
    <Compile Update="Behaviors\InteractionTrackerGesture\InteractionTrackerGesture.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
    <Compile Update="Behaviors\LayoutBehavior.cs">
      <Generator>MSBuild:Compile</Generator>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Folder Include="AnimatableNodes\" />
  </ItemGroup>
</Project>
