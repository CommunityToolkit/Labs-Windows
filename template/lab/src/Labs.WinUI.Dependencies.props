<Project>
    <Import Project="$(RepositoryDirectory)\common\Labs.TargetFrameworks.props"/>

    <PropertyGroup>
        <IsUwpTargetFramework Condition="'$(TargetFramework)' == '$(UwpTargetFramework)'">True</IsUwpTargetFramework>
        <IsWinAppSdkTargetFramework Condition="'$(TargetFramework)' == '$(WinAppSdkTargetFramework)'">True</IsWinAppSdkTargetFramework>

        <IsUnoTargetFramework Condition="'$(IsWinAppSdkTargetFramework)' != 'True' AND '$(IsUwpTargetFramework)' != 'True'">True</IsUnoTargetFramework>
        <UseWinUI>2</UseWinUI>
    </PropertyGroup>

    <Import Project="Dependencies.WinUI2.props" />
    <Import Project="Dependencies.WinUI3.props" />
</Project>
